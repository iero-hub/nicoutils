{
  "name": "NiconicoPremiumLinkKiller",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Chrome Extension to delete premium link on seek bar.",
  "content_scripts": [{
    "matches": ["https://www.nicovideo.jp/watch/*"],
    "js": [
      "content.js"
    ],
    "run_at": "document_end",
    "all_frames": true
  }]
}